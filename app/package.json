    "razzle-dev-utils": "^4.2.14",
    "razzle-plugin-manifest": "^4.2.14",
    "sass": "1.42.1",
    "ts-loader": "8",
    "typescript": "4.4.3",
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
