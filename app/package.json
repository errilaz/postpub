    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-helmet-async": "1.2.2",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@jaredpalmer/after": "^3.2.0",
    "@poststack/cli": "workspace:*",
